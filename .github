function tp(x,y,z)
   local tween,err = pcall(function()
       local tween = game:GetService("TweenService"):Create(game.Players.LocalPlayer.Character["HumanoidRootPart"],TweenInfo.new(1,Enum.EasingStyle.Quad),{CFrame=CFrame.new(x,y,z)});
